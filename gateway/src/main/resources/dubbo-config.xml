<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo
       http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:reference id="userServiceInterface"
                     check="false"
                     interface="com.gislab.zhsou.common.service.api.UserServiceInterface"/>

    <dubbo:reference id="userFavoriteInterface"
                     check="false"
                     interface="com.gislab.zhsou.common.service.api.UserFavoriteInterface"/>

    <dubbo:reference id="articleInterface"
                     check="false"
                     interface="com.gislab.zhsou.common.service.api.ArticleInterface"/>
</beans>